{
  "name": "eslint-config-breki",
  "version": "1.0.20",
  "description": "A handful of rules used by Breki Tomasson for formatting, error-checking and practicing Safe Hex.",
  "homepage": "https://github.com/BrekiTomasson/eslint-config-breki#readme",
  "license": "MIT",
  "scripts": {
    "patch": "npm version patch && npm publish && git push --follow-tags",
    "lint": "eslint \"./**/*.js\"",
    "test": "jest",
    "codacy": "cat ./test/coverage/lcov.info | codacy-coverage",
    "travis-deploy-once": "travis-deploy-once"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrekiTomasson/breki-eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "standard"
  ],
  "author": {
    "name": "Breki Tomasson",
    "email": "breki.tomasson@gmail.com",
    "url": "http://brekitomasson.com"
  },
  "bugs": {
    "url": "https://github.com/BrekiTomasson/eslint-config-breki/issues"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-array-func": "^3.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-es": "^1.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-ocd": "^1.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.4.0",
    "eslint-plugin-you-dont-need-momentjs": "^1.4.0",
    "npm-install-package": "^2.1.0",
    "remark-lint": "^6.0.4",
    "resolve": "^1.9.0",
    "standard-engine": "^10.0.0"
  },
  "devDependencies": {
    "codacy-coverage": "^3.4.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-runner-eslint": "^0.7.1",
    "lint-staged": "^8.1.0",
    "travis-deploy-once": "^5.0.11"
  },
  "jest": {
    "projects": [
      "./test/jest-eslint.config.js",
      "./test/jest-test.config.js"
    ]
  }
}
